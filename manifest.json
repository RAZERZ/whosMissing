{
  "name": "whosMissing",
  "version": "1.0",
  "description": "Applications include checking who is in a Google Meet meeting through JSON.",

  "permissions": [
    "storage",
    "declarativeContent",
    "activeTab"
  ],

  "background": {
    "scripts": [
      "background/eventListener.js"
    ],
    "persistent": false
  },

  "icons": {
    "32": "img/logo/whosMissing.png"
  },

  "page_action": {
    "default_popup": "index.html"
  },

  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",

  "manifest_version": 2
}